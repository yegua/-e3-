/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.ui-container {
    width: 100%;
    height: 100%;
    background-image: url(/images/login_bg.png);
    background-size: cover;
}
#mobile-num {
    background: url(../../images/login_phone.png) no-repeat left center;
    background-size: 40rpx;
}
#user-name {
    background: url(../../images/login_user.png) no-repeat left center;
    background-size: 40rpx;
}
#psw-info {
    background: url(../../images/login_password.png) no-repeat left center;
    background-size: 40rpx;
}